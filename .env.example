# Kumar Prescod Boxing - Environment Variables

# App Configuration
REACT_APP_APP_NAME="Kumar Prescod Boxing"
REACT_APP_VERSION="1.0.0"
REACT_APP_ENVIRONMENT="development"

# API Configuration
REACT_APP_API_BASE_URL="https://api.kumarprescod.com"
REACT_APP_API_TIMEOUT="10000"

# Analytics & Monitoring
REACT_APP_GA_MEASUREMENT_ID="G-XXXXXXXXXX"
REACT_APP_FACEBOOK_PIXEL_ID="123456789012345"

# Error Reporting & Monitoring
REACT_APP_ERROR_REPORTING_ENABLED="false"
REACT_APP_ERROR_REPORTING_ENDPOINT="https://errors.kumarprescod.com/api/errors"
REACT_APP_REAL_USER_MONITORING_ENABLED="false"

# Payment Integration
REACT_APP_STRIPE_PUBLISHABLE_KEY="pk_test_..."

# Social Media
REACT_APP_TWITTER_HANDLE="@kumarprescod"
REACT_APP_INSTAGRAM_HANDLE="@kumarprescod"
REACT_APP_YOUTUBE_CHANNEL="@kumarprescod"

# Fight Data
REACT_APP_NEXT_FIGHT_DATE="2025-08-16"
REACT_APP_NEXT_FIGHT_LOCATION="Oakland, CA"
REACT_APP_NEXT_FIGHT_VENUE="Oakland Arena"

# Feature Flags
REACT_APP_ENABLE_SHOP="true"
REACT_APP_ENABLE_PODCAST="true"
REACT_APP_ENABLE_MEMBER_AREA="true"
REACT_APP_ENABLE_ADMIN_AREA="false"

# Development
REACT_APP_MOCK_API="false"
REACT_APP_DEBUG_PERFORMANCE="false"
REACT_APP_DEBUG_ANALYTICS="false"

# CDN & Assets
REACT_APP_CDN_BASE_URL="https://cdn.kumarprescod.com"
REACT_APP_IMAGES_BASE_URL="https://images.kumarprescod.com"

# Cache Configuration
REACT_APP_CACHE_VERSION="1.0"
